<?xml version="1.0" ?>
<resource>
  <object class="wxDialog" name="frmPassword">
    <title>Change Password</title>
    <size>270, 130</size>
    <style>wxDIALOG_MODAL|wxCAPTION|wxSYSTEM_MENU</style>
    <object class="wxTextCtrl" name="txtCurrent">
      <pos>110, 10</pos>
      <size>155, 23</size>
      <style>wxTE_PASSWORD</style>
    </object>
    <object class="wxTextCtrl" name="txtNew">
      <pos>110, 40</pos>
      <size>155, 23</size>
      <style>wxTE_PASSWORD</style>
    </object>
    <object class="wxTextCtrl" name="txtConfirm">
      <pos>110, 70</pos>
      <size>155, 23</size>
      <style>wxTE_PASSWORD</style>
    </object>
    <object class="wxStaticText" name="lblCurrent">
